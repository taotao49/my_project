<view class="bar_goods_nav"><view class="cart-sub-left"><block wx:for="{{options}}" wx:for-item="obj" wx:for-index="idx" wx:key="idx"><view data-event-opts="{{[['tap',[['onClick',[idx,'$0'],[[['options','',idx]]]]]]]}}" class="cart-button-left" bindtap="__e"><view class="icon"><uni-icons vue-id="{{'58426c44-1-'+idx}}" type="{{obj.icon}}" size="20" color="#646566" bind:__l="__l"></uni-icons></view><text class="text">{{obj.text}}</text><view class="dot-box"><block wx:if="{{obj.info}}"><text class="{{['dot','',(obj.info>9)?'dots':'']}}" style="{{'background-color:'+(obj.infoBackgroundColor?obj.infoBackgroundColor:'#ff0000')+';'+('color:'+(obj.infoColor?obj.infoColor:'#fff')+';')}}">{{obj.info}}</text></block></view></view></block></view><block wx:if="{{cart_action}}"><view class="{{['cart-sub-right','',(isEllipse)?'right':'']}}"><block wx:for="{{buttonGroup}}" wx:for-item="obj" wx:for-index="idx" wx:key="idx"><view data-event-opts="{{[['tap',[['buttonClick',[idx,'$0'],[[['buttonGroup','',idx]]]]]]]}}" class="cart-button-right" style="{{'background-color:'+(obj.backgroundColor)+';'+('color:'+(obj.color)+';')}}" bindtap="__e"><text class="cart-button-right-text" style="{{'color:'+(obj.color)+';'}}">{{obj.text}}</text></view></block></view></block></view>
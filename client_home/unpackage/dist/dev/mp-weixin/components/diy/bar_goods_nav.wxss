.bar_goods_nav{display:flex;flex:1;height:50px;background-color:#fff;z-index:999999}.bar_goods_nav .cart-sub-left{display:flex;padding:0 5px}.bar_goods_nav .cart-sub-right{display:flex;flex:1}.bar_goods_nav .right{margin:5px 0;margin-right:10px;border-radius:100px;overflow:hidden}.bar_goods_nav .cart-button-left{display:flex;position:relative;justify-content:center;align-items:center;flex-direction:column;margin:0 10px}.bar_goods_nav .icon{width:18px;height:18px}.bar_goods_nav .image{width:18px;height:18px}.bar_goods_nav .text{margin-top:3px;font-size:12px;color:#646566}.bar_goods_nav .cart-button-right{display:flex;flex-direction:column;display:flex;flex:1;justify-content:center;align-items:center}.bar_goods_nav .cart-button-right-text{font-size:14px;color:#fff}.bar_goods_nav .cart-button-right:active{opacity:.7}.bar_goods_nav .dot-box{flex-direction:column;display:flex;position:absolute;right:-2px;top:2px;justify-content:center;align-items:center}.bar_goods_nav .dot{padding:0 4px;line-height:15px;color:#fff;text-align:center;font-size:12px;background-color:red;border-radius:15px}.bar_goods_nav .dots{padding:0 4px;border-radius:15px}